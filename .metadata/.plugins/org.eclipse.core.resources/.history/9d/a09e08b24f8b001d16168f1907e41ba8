package phan02;
/**
 * 
 * 6/ Thao tác với toán tử điều kiện
 * @author
 * 
 * */
public class Bai06 {
	public static void main(String[] args) {
		int value1 = 1;
		int value2 = 2;
		int result;
		boolean	someCondition = true;
		result = someCondition ? value2: value1;
		System.out.println(result);
	}
}
