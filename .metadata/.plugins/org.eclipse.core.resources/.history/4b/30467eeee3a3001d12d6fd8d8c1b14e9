package phan03;

public class Bai05 {
	public static void main(String[] args) {
		
	}
}

public int USCLN(int a, int b) {
    int temp1 = a;
    int temp2 = b;
    while (temp1 != temp2) {
        if (temp1 > temp2) {
            temp1 -= temp2;
        } else {
            temp2 -= temp1;
        }
    }
    int uscln = temp1;
    return uscln;
}