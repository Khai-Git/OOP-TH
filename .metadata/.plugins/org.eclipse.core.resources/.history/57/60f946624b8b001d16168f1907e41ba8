package phan02;
/*
 * 
 * 1/ Thao tác với toán tử số học
 * 
 * */
public class Bai01 {
	public static void main(String[] args) {
		int result = 1 + 2;
		result = result - 1;
		System.out.println("Final result: " + result);
		result = result * 2;
		System.out.println("Final result: " + result);
		result = result / 2;
		System.out.println("Final result: " + result);
		result = result + 8;
		System.out.println("Final result: " + result);
		result = result % 7;
		System.out.println("Final result: " + result);
	}
}
