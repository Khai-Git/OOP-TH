package phan02;
/**
/*
 * 
 * 3/ Thao tác với toán tử 1 ngôi
 * 
 * */
public class Bai03 {
	public static void main(String[] args) {
		int result = + 1;
		System.out.println(result);
		
		result --;
		System.out.println(result);
		
		result ++;
		System.out.println(result);
		
		result = -result;
		System.out.println(result);
		
		boolean success = false;
		System.out.println(success);
		System.out.println(!success);
	}
}
